# Computes prime numbers by trial subtraction
display = 2;
my candidate = 3;
my divisor;
my remainder;
do infinite loop {
  divisor = 1;
  do trials {
    divisor += 2;
    remainder = candidate;
    do repeated subtraction {
      remainder -= divisor;
    } while (remainder > 0);
  } while (remainder < 0);
  if (divisor == candidate) prime found {
    display = candidate;
  }
  candidate += 2;
} while (1);
